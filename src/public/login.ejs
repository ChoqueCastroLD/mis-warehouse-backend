<%- include('layouts/header') %>

<div class="row">
    <div class="input-field col s12">
        <h3>Warehouse - <%= title %></h3>
    </div>
    <div class="input-field col s12">
        <input id="txtLoginEmail" placeholder="sample@gmail.com" type="text" class="validate">
        <label for="first_name">Email</label>
    </div>
    <div class="input-field col s12">
        <input id="txtLoginPass" type="password" class="validate">
        <label for="last_name">Password</label>
    </div>
    <hr>
    <div class="input-field col s12">
        <button id="btnLoginSubmit" class="btn waves-effect waves-light" type="submit" name="action">Submit
            <i class="material-icons right">send</i>
        </button>
    </div>
</div>
</div>

<script src="/js/login.js"></script>

<%- include('layouts/footer') %>